{
  "name": "zeyra",
  "version": "2.2.0",
  "description": "Client-side WebCrypto helpers for AES-GCM encryption, ECDSA signatures, RSA-OAEP key wrapping, HMAC, key generation, and byte-oriented cluster helpers.",
  "keywords": [
    "webcrypto",
    "aes-gcm",
    "ecdsa",
    "hmac",
    "rsa-oaep",
    "jwk",
    "encryption",
    "signature",
    "key-wrapping",
    "wrapping",
    "unwrapping",
    "cluster",
    "keyset",
    "compression",
    "bytecodec",
    "typescript"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node --test test.js && node test.js --bench",
    "bench": "npm run build && node test.js --bench",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-base/zeyra.git"
  },
  "bugs": {
    "url": "https://github.com/z-base/zeyra/issues"
  },
  "homepage": "https://github.com/z-base/zeyra#readme",
  "dependencies": {
    "bytecodec": "^3.0.0"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false
}
